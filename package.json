    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "dockerlint": "^0.3.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "express": "^4.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
