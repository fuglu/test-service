{
  "name": "test-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --inspect-brk -- src/index.ts",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.5",
    "@types/express": "^4.16.1",
    "dockerlint": "^0.3.9",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "express": "^4.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "Dockerfile": [
      "dockerlint -p",
      "git add"
    ],
    "*.{json,yml,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
