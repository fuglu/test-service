{
  "name": "test-service",
  "version": "1.0.0",
  "description": "Tiny express service",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuglu/test-service.git"
  },
  "keywords": [
    "typescript",
    "express",
    "terminus",
    "eslint",
    "prettier",
    "jest",
    "supertest",
    "husky",
    "lint-staged"
  ],
  "author": {
    "name": "Benjamin Kluck",
    "email": "ben@fug.lu",
    "url": "https://fuglu.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuglu/test-service/issues"
  },
  "homepage": "https://github.com/fuglu/test-service#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "dockerlint": "^0.3.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "Dockerfile": [
      "dockerlint -p",
      "git add"
    ]
  }
}
